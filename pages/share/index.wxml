<view class="category">
  <view class="sousuo">
    <van-search style="width:100%" value="{{ value }}" placeholder="请输入搜索关键词" />
  </view>
  <view class="biaoqiao">
    <van-tag
      wx:if="{{ show.primary }}"
      closeable
      round
      color="#EB607D"
      size="large"
      type="primary"
      id="primary"
      bind:close="onClose"
      custom-class="biaoqian_title"
    >标签</van-tag>
    <van-tag
      wx:if="{{ show.success }}"
      closeable
      round
      color="#EB607D"
      size="large"
      type="primary"
      id="success"
      bind:close="onClose"
      custom-class="biaoqian_title"
    >标签</van-tag>
  </view>
  <image src="../../images/fybanner.png" bindload="imageLoad" mode='aspectFill' style='width:100%;height:{{ images.height }}px;'></image>

