/* pages/yiyuan/list.wxss */
page{
  font-family: SimSun;
  background-color: #EEEEEE;
}
.category{
  display: grid;
}

.list{ width: 100%; padding-top: 20rpx; box-sizing:border-box;}
.yyfy_li{ width: 100%; padding: 20rpx; margin-bottom: 20rpx; position: relative; display:flex; justify-content: space-around;  background-color: #FFFFFF; box-sizing:border-box;}
.yyfy_li_tb{ height: 60rpx; width: 46rpx; position: absolute; top: 0;left: 0;}
.yyfy_li_v{ height: 39rpx; width: 32rpx; position: absolute; top: 20rpx;right: 30rpx;}
.yyfy_li_img{ height: 240rpx; width: 200rpx;border-radius:10px;}
.yyfy_li_content{ height: 240rpx; width: 450rpx; padding: 10rpx; box-sizing:border-box;}
.yyfy_li_content_title{ height: 80rpx; width: 360rpx; line-height: 40rpx; overflow: hidden;}

.yyfy_li_content_money{height: 80rpx; width: 100%; line-height: 80rpx; display:flex; justify-content: space-around; background-color:#fff;}

.yyfy_li_content_note{ height: 80rpx; width: 100%; line-height: 60rpx; display:flex; justify-content: space-around;}
.yyfy_content_des{ width: 280rpx; font-size: 24rpx;}
.yyfy_content_des_fy{ line-height: 40rpx; color: #EA617B;}
.yyfy_content_des_ads{ line-height: 30rpx; color: #8D8B8C;}
.yyfy_zx{ border-radius:20px; height: 60rpx; background-color: #EA617B; color: white; font-size: 24rpx; font-weight: 500;}


.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.block {
  width: 60%;
  padding: 20rpx; box-sizing:border-box;
  background: #fff;
  padding-bottom: 4rpx;
}

.block-img{width: 100%;}




