                <van-button custom-style="width:200rpx;font-weight:500;" bindtap='onGetInfo' data-val="{{vo.id}}" color="#EA617B" size="small" round type="info">分享赚|自购省</van-button>
              </view>
              <view class="yyfy_li_content_note">
                
