        <swiper-item>
          <image class="swiper-item" src="{{item}}"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <van-notice-bar style="width:100%;" color="#242424" background="#E2E2E2" left-icon="volume-o" text="在代码阅读过程中人们说脏话的频率是衡量代码质量的唯一标准。"/>
  
