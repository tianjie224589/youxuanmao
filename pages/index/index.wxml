<van-overlay show="{{ orderPopup }}" bind:click="onClickHide">
  <view class="wrapper">
    <view class="block">
      <view class="block-title">
        <van-button type="default" color="#EB617B" bindtap="onClickHide">继续逛逛</van-button>
      </view>
      <view class="block-des" bindtap="onToOrderList">查看订单 ></view>
    </view>
  </view>
</van-overlay>
