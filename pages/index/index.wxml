<view class="container">
  <view class="top">
    <view class="address">
      <navigator url="map">重庆<van-icon size="14px" name="arrow-down" /></navigator>
    </view>
    <view class="page_bd">
      <van-search class="search" background="#E8627B" placeholder="搜索" readonly="true" bindtap='onSearch' value="{{ searchvalue }}"></van-search>
    </view>
  </view>
  
  <view class="page-section page-section-spacing swiper">
